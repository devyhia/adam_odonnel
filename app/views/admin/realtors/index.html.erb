<div class='pull-right'>
  <%= link_to 'New Realtor', new_admin_realtor_path, :class=>"btn btn-primary btn-lg" %>
</div>
<h1>Realtors</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_realtors.each do |admin_realtor| %>
      <tr>
        <td><%= admin_realtor.title %></td>
        <td><%= admin_realtor.url %></td>
        <td class='text-right'>
          <%= link_to 'Show', admin_realtor_path(admin_realtor), :class=>"btn btn-xs btn-default" %>
          <%= link_to 'Edit', edit_admin_realtor_path(admin_realtor), :class=>"btn btn-xs btn-default" %>
          <%= link_to 'Destroy', admin_realtor_path(admin_realtor), method: :delete, data: { confirm: 'Are you sure?' }, :class=>"btn btn-xs btn-default" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
